<mvc:View
        displayBlock="true"
        controllerName="tmp_demo.controller.Home.Home"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:table="sap.ui.table"
        xmlns:l="sap.ui.layout"
	      xmlns:f="sap.ui.layout.form"
        xmlns="sap.m"
        xmlns:core="sap.ui.core">

  <Page
          showHeader="true"
          enableScrolling="true"
          class="sapUiContentPadding">
    
    <VBox class="sapUiSmallMargin" alignItems="Center">
      <f:SimpleForm id="SimpleFormChange354"
        editable="true"
        layout="ResponsiveGridLayout"
        title=""
        labelSpanXL="3"
        labelSpanL="3"
        labelSpanM="3"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="4"
        emptySpanL="4"
        emptySpanM="4"
        emptySpanS="0"
        columnsXL="1"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false" >
        <f:content>
          <Label text="User Name" />
          <Input id="name" value="{SupplierName}" />
          <Label text="Password" />
          <Input value="{Street}" type="Password"/>

        
          <Label text="" />
          <Select id="country" selectedKey="{Country}">
            <items>
              <core:Item text="Admin" key="1"/>
              <core:Item text="User" key="2"/>
            </items>
          </Select>
          <Button text="Login" press="onUserNav"></Button>
        </f:content>
      </f:SimpleForm>
    </VBox>
    
  </Page>

</mvc:View>
